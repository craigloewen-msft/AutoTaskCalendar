{
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "axios": "^1.13.2",
    "connect-ensure-login": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "es6-promise": "^4.2.8",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "googleapis": "^166.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.19.3",
    "nodemon": "^3.1.11",
    "passport": "^0.7.0",
    "passport-local-mongoose": "^8.0.0"
  },
  "scripts": {
    "build": "cd webinterface; npm install; npm run build; cd ..; rm -rf ./dist; mv ./webinterface/dist .",
    "dev": "concurrently --kill-others \"nodemon --inspect app.js\" \"cd webinterface; npm run serve\"",
    "seed": "node seedDatabase.js",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:start-servers": "./start-test-servers.sh",
    "test:stop-servers": "./stop-test-servers.sh",
    "test:ci": "./start-test-servers.sh && playwright test; TEST_EXIT=$?; ./stop-test-servers.sh; exit $TEST_EXIT"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "concurrently": "^9.2.1",
    "playwright": "^1.56.1"
  }
}
